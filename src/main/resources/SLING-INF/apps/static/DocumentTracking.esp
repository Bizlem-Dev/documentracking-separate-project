<script type="text/javascript">
        $(document).ready(function(){
            $('.metismenu li a').click(function(){
                $('.metismenu li').removeClass('active1');
                $('.metismenu li a').removeClass('active1');
                $(this).addClass('active1');
                $(this).parent('li').addClass('active1');
            });
        });

